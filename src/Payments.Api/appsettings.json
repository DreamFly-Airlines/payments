{
  "Kafka": {
    "PaymentsEvents": {
      "ProducerSettings": {
        "BootstrapServers": "localhost:9092",
        "ClientId": "payments-producer",
        "Acks": "All",
        "EnableIdempotence": "true"
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
